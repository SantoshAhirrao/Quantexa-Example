  "scripts": {
    "ng": "ng",
    "start": "parallelshell \"ws -c lws.config.js\" \"ws -c lws-login.config.js\"",
    "start-kube": "parallelshell \"ws -c lws.config.kubernetes.js\" \"ws -c lws-login.config.js\"",
    "build": "ng build --prod --stats-json",
    "build-login": "ng build --prod --stats-json login",
    "watch": "ng build --watch --preserve-symlinks --ts-config ./src/tsconfig.app.dev.json",
    "watch-login": "ng build --watch --aot login --ts-config ./src-login/tsconfig.app.dev.json",
    "watch-all": "parallelshell \"npm run watch\" \"npm run watch-login\"",
    "test": "ng test explorer --source-map=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock-server": "ws -c mock-server/lws.config.js",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
